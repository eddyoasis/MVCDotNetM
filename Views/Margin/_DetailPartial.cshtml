@model MarginViewModel

<div class="modal-header">
    <h5 class="modal-title">Details Item</h5>
    <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
        <span>&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="form-group">
        <label asp-for="Username" class="control-label col-md-2"></label>
        <div class="col-md-10">
            <input asp-for="Username" class="form-control" disabled />
            <span asp-validation-for="Username" class="text-danger"></span>
        </div>
    </div>

    <br />
    <div class="form-group">
        <label asp-for="Percentage" class="control-label col-md-2"></label>
        <div class="col-md-10">
            <input asp-for="Percentage" class="form-control" disabled />
            <span asp-validation-for="Percentage" class="text-danger"></span>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-info" data-bs-dismiss="modal">Close</button>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
